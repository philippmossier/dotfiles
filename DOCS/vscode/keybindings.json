[
  // ----- File explorer ----
  {
    "key": "ctrl+enter",
    "command": "filesExplorer.openFilePreserveFocus",
    "when": "filesExplorerFocus"
  },
  {
    "key": "ctrl+a",
    "command": "explorer.newFile",
    "when": "filesExplorerFocus"
  },
  {
    "key": "ctrl+r",
    "command": "renameFile",
    "when": "filesExplorerFocus"
  },
  {
    "key": "ctrl+s",
    "command": "explorer.openToSide", // splits it only when only 1 window open (does not get over 2 splits)
    "when": "filesExplorerFocus"
  },
  // {
  // 	// alreadt default (just explicitly added for reference) (can be deleted without effect)
